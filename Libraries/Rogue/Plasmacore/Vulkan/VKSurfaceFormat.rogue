module Plasmacore

class VKSurfaceFormat [compound]
  PROPERTIES
    native "VkSurfaceFormatKHR value;"

  METHODS
    method bit_format->VKBitFormat
      return VKBitFormat( native("$this.value.format")->Int32 )

    method color_space->VKColorSpace
      return VKColorSpace( native("$this.value.colorSpace")->Int32 )

    method to->String
      return "($ $)"(bit_format,color_space)
endClass
