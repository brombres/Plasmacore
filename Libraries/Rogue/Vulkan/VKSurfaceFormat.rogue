module Vulkan

class VKSurfaceFormat( native_format:VKNativeSurfaceFormat ) [compound]
  METHODS
    method format->VKFormat
      return VKFormat( native("$native_format.value.format")->Int32 )

    method color_space->VKColorSpace
      return VKColorSpace( native("$native_format.value.colorSpace")->Int32 )

    method to->String
      return "($ $)"(format,color_space)
endClass
